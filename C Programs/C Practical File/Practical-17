#include <stdio.h>

int main() {
int num1, num2, max;
printf("Enter first number: ");
scanf("%d", &num1);
printf("Enter second number: ");
scanf("%d", &num2);
 // Use conditional operator to find the maximum
max = (num1 > num2) ? num1 : num2;
printf("Maximum: %d\n", max);
printf("Aditya Mehta BCA Shift-1 Section A\n");
return 0;
}
